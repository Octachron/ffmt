(jbuild_version 1)

(executable (
            (name interpolation)
            (modules (Interpolation))
            (preprocess (pps (ppxi)))
            (libraries (freefmt))
))


(executable (
            (name meta)
            (modules (Meta))
            (preprocess (pps (ppx)))
            (libraries (freefmt))
))


(alias
 ((name    runtest)
  (deps    (interpolation.exe))
  (action  (run ${<}))
))


(alias
 ((name    runtest)
  (deps    (meta.exe))
  (action  (run ${<}))
))
